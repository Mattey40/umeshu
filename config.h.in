#ifndef __CONFIG_H_INCLUDED__
#define __CONFIG_H_INCLUDED__ 

/* Defined if fpu_control.h is present in the system. */
#cmakedefine HAVE_FPU_CONTROL_H       1

/* Defined if C preprocessor knows the warning directive */
#cmakedefine CPP_HAS_WARNING          1

/* Defined if the user chose to use fast, but not robust version of the
 * geometric predicates. Can be changed by running ccmake. */
#cmakedefine USE_FAST_GEOMETRIC_TESTS 1

/* Define to the address where bug reports for this package should be sent. */
#cmakedefine PACKAGE_BUGREPORT "@PACKAGE_BUGREPORT@"

/* Define to the full name of this package. */
#cmakedefine PACKAGE_NAME "@PACKAGE_NAME@"

/* Define to the full name and version of this package. */
#cmakedefine PACKAGE_STRING "@PACKAGE_STRING@"

/* Define to the one symbol short name of this package. */
#cmakedefine PACKAGE_TARNAME "@PACKAGE_TARNAME@"

/* Define to the home page for this package. */
#cmakedefine PACKAGE_URL "@PACKAGE_URL@"

/* Define to the version of this package. */
#cmakedefine PACKAGE_VERSION "@PACKAGE_VERSION@"

#endif /* __CONFIG_H_INCLUDED__ */
